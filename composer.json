{
  "name": "spotonlive/laravel-google-ads",
  "type": "library",
  "description": "Google Ads API for Laravel",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "keywords": [
    "laravel",
    "ads",
    "adwords",
    "google"
  ],
  "homepage": "https://github.com/nikolajlovenhardt/laravel-google-ads",
  "license": "MIT",
  "authors": [
    {
      "name": "Nikolaj Petersen",
      "email": "nikolaj.lovenhardt@gmail.com",
      "homepage": "http://github.com/nikolajlovenhardt"
    }
  ],
  "require": {
    "php": ">=7.2",
    "ext-soap": "*",
    "illuminate/console": "^6.0|^7.0",
    "illuminate/support": "^6.0|^7.0",
    "googleads/googleads-php-lib": "^41.0|^49.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5.8|^9.3.3"
  },
  "autoload": {
    "psr-4": {
      "LaravelGoogleAds\\": "src/LaravelGoogleAds"
    },
    "files": [
      "src/LaravelGoogleAds/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "LaravelGoogleAdsTests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "LaravelGoogleAds\\LaravelGoogleAdsProvider"
      ]
    }
  }
}
